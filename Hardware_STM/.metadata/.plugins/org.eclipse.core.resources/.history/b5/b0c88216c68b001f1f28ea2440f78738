/* USER CODE BEGIN PV */
// Buffers
uint8_t buffer[100];

// For the ultrasonic sensor
uint32_t tc1 = 0, tc2 = 0, echo = 0;
float dist = 0.0;
bool is_first_captured = false;

// Sensors
sensor_t sensor;

// Count the number of commands
uint8_t cmd_buffer[1000];
memset(cmd_buffer, 0, 8000));
uint8_t cmd_cnt = 0;
uint8_t receive[1];
uint8_t head = 0, curr = -1;
uint8_t current_cmd = 0;
