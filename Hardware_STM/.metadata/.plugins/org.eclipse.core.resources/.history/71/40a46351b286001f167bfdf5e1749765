#include "test.h"

void test() {
	// Abhinav's Task: left - right - right - backward - repeat
	for (int i = 0; i < 3; i++) {
	  turn_right(90);
	  HAL_Delay(1000);
	  servo_set_val(STRAIGHT);
	  HAL_Delay(1000);

	  turn_left(90);
	  turn_left(90);
	  HAL_Delay(2000);
	  motor_backward_inf();
	  HAL_Delay(3000);
	  motor_stop();
	  HAL_Delay(2000);
}

}

